   "source": [
    "scores = [80, 89, 99, 83] # -> 87.75\n",
    "\n",
    "sum_scores = sum(scores)\n",
    "sum_count = len(scores)\n",
    "\n",
    "avg_scores = sum_scores / sum_count\n",
    "print(avg_scores)\n",
    "\n",
    "result = 0 # 누적 점수의 합을 저장하는 변수\n",
    "for score in scores:\n",
    "    result += score #\n",
    "print(result / len(scores))"
   ]
